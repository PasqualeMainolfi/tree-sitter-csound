{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "csound",
      "camelcase": "Csound",
      "title": "Tree-sitter Csound",
      "scope": "source.csound",
      "file-types": [
        "orc",
        "sco",
        "udo",
        "csd"
      ],
      "injection-regex": "^csound$",
      "class-name": "TreeSitterCsound"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Csound grammar for the tree-sitter parser",
    "authors": [
      {
        "name": "Pasquale Mainolfi",
        "email": "mnlpql@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-csound"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}