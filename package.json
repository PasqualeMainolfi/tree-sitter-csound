{
  "name": "tree-sitter-csound",
  "version": "0.1.1",
  "description": "Csound grammar for the tree-sitter parser",
  "repository": "https://github.com/tree-sitter/tree-sitter-csound",
  "license": "MIT",
  "author": {
    "name": "Pasquale Mainolfi",
    "email": "mnlpql@gmail.com"
  },
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "csound"
  ],
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm",
    "tree-sitter-csound.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.5.0",
    "node-gyp-build": "^4.8.4",
    "web-tree-sitter": "^0.26.3"
  },
  "devDependencies": {
    "prebuildify": "^6.0.1",
    "tree-sitter": "^0.22.4",
    "tree-sitter-cli": "^0.25.10"
  },
  "peerDependencies": {
    "tree-sitter": "^0.22.4"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "node --test bindings/node/*_test.js",
    "build:wasm": "tree-sitter build --wasm",
    "prepublishOnly": "tree-sitter run build:wasm"
  }
}
